# Source Prezto.
if [[ -s "${ZDOTDIR:-$HOME}/.zprezto/init.zsh" ]]; then
  source "${ZDOTDIR:-$HOME}/.zprezto/init.zsh"
fi

# Customize to your needs...
SYSTEM=$( uname )

if [[ $SYSTEM -eq "Darwin" ]]; then
    unalias run-help
    autoload run-help
    HELPDIR=/usr/local/share/zsh/help
fi
